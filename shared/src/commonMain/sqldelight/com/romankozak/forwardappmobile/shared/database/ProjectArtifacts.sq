CREATE TABLE project_artifacts (
  id TEXT NOT NULL PRIMARY KEY,
  projectId TEXT NOT NULL,
  content TEXT NOT NULL,
  createdAt INTEGER NOT NULL,
  updatedAt INTEGER NOT NULL
);

getArtifactForProject:
SELECT * FROM project_artifacts WHERE projectId = ?;

insertProjectArtifact:
INSERT OR REPLACE INTO project_artifacts (id, projectId, content, createdAt, updatedAt) VALUES (?, ?, ?, ?, ?);

deleteProjectArtifact:
DELETE FROM project_artifacts WHERE id = ?;
