getArtifactForProject:
SELECT *
FROM project_artifacts
WHERE projectId = ?1
LIMIT 1;

insertProjectArtifact:
INSERT OR REPLACE INTO project_artifacts(
  id,
  projectId,
  content,
  createdAt,
  updatedAt
) VALUES (?1, ?2, ?3, ?4, ?5);

deleteProjectArtifact:
DELETE FROM project_artifacts
WHERE id = ?1;
