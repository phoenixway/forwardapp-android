selectAll:
SELECT *
FROM project_execution_logs;

selectByProjectId:
SELECT *
FROM project_execution_logs
WHERE projectId = ?
ORDER BY timestamp DESC;

insert:
INSERT OR REPLACE INTO project_execution_logs(id, projectId, timestamp, type, description, details)
VALUES (?, ?, ?, ?, ?, ?);

deleteById:
DELETE FROM project_execution_logs
WHERE id = ?;

deleteAll:
DELETE FROM project_execution_logs;
